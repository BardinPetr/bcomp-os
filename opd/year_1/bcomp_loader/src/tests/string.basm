ORG 0x10

INCLUDE util/math.basm
INCLUDE util/string.basm
INCLUDE util/array.basm
INCLUDE io/output.basm
INCLUDE io/input.basm
INCLUDE os/memory.basm

A: WORD "abcd"
B: WORD 10 DUP (0x11)
B_PTR: WORD $B

START:
  LD B_PTR
  CALL $READ_LINE

  CALL $PRINT_ENDL
  LD B_PTR
  CALL $PRINTLN
  
  LD A_PTR
  PUSH
  LD B_PTR
  CALL $STRCMP
  ADD #0x30
  CALL $PRINT_CHAR

  POP
  JUMP START

_SYS__CODE_END: WORD ?